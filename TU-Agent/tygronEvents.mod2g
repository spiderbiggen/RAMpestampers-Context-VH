use agentKnowledge as knowledge.

module tygronEvents {
	%%%%% Percept Processing %%%%%
	if percept(stakeholders(X)), bel(stakeholders(Y)) then insert(stakeholders(X)) + delete(stakeholders(Y)).
	if percept(settings(X)), bel(settings(Y)) then insert(settings(X)) + delete(settings(Y)).
	if percept(functions(X)), bel(functions(Y)) then insert(functions(X)) + delete(functions(Y)).
	if percept(buildings(X)), bel(buildings(Y)) then insert(buildings(X)) + delete(buildings(Y)).
	if percept(indicators(X)), bel(indicators(Y)) then insert(indicators(X)) + delete(indicators(Y)).
	if percept(lands(X)), bel(lands(Y)) then insert(lands(X)) + delete(lands(Y)).
	if percept(zones(X)), bel(zones(Y)) then insert(zones(X)) + delete(zones(Y)).
	if percept(requests(X)), bel(requests(Y)) then insert(requests(X)) + delete(requests(Y)).
	if percept(actions(X)), bel(actions(Y)) then insert(actions(X)) + delete(actions(Y)).
	
	%Update indicatorlink%
	if percept(stakeholders(X)), bel(member([stakeholder(3, _, Budget, _), indicatorLink(_ ,Weights)], X))
		then insert(indicatorlink(Weights)).
}
