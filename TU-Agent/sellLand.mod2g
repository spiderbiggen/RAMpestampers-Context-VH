use agentKnowledge as knowledge.
use tygron as actionspec.
use sellLandHandler as module.

order=linear.

module sellLand {
	
	%for the emptySpace that has no building on it, try selling it to the Municipality.
	%landToSell holds a list of all stakeholders we've proposed this sell to already, we'll add our own stakeholder in advance
	if bel(emptySpace(MultiPolygon, Area, false), generousPrice(Price, Area))
		then insert(landToSell(MultiPolygon, Price, [3])) + delete(emptySpace(MultiPolygon, Area, false)) + sellLandHandler.
}