order=linearall.

module upgrade {
	
	if bel(buildings(Buildings), upgrades(Upgrades), 
		member(building(ID,_, 3, _, _, SrcID, _, MultiPolygon), Buildings), 
		member([_, UpgradeID, SrcID], Upgrades))
			then building_plan_upgrade(UpgradeID, MultiPolygon).
}
